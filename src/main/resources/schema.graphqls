type Query {
  eventById(id: ID!): Event
  seriesById(id: ID!): Series
}

type Event {
  id: ID!
  title: String!
  description: String
  "Duration in seconds"
  duration: Float!
  series: Series
}

"A series. Can contain multiple events."
type Series {
  id: ID!
  name: String!
  description: String
}
